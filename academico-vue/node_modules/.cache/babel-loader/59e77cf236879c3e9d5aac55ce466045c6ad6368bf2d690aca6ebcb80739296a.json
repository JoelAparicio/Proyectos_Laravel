{"ast":null,"code":"import axios from 'axios';\nimport { confirmar } from '../funciones';\nexport default {\n  data() {\n    return {\n      estudiantes: null,\n      cargando: false\n    };\n  },\n  mounted() {\n    this.getEstudiantes();\n  },\n  methods: {\n    getEstudiantes() {\n      this.cargando = true;\n      axios.get('http://academicobackend.test/api/v1/estudiantes').then(res => {\n        this.estudiantes = res.data;\n        this.cargando = false;\n      });\n    },\n    eliminar(id, nombre) {\n      confirmar('http://academicobackend.test/api/v1/estudiantes/', id, 'Eliminar registro', 'Realmente desea eliminar a ' + nombre + '?');\n      this.cargando = false;\n    }\n  }\n};","map":{"version":3,"names":["axios","confirmar","data","estudiantes","cargando","mounted","getEstudiantes","methods","get","then","res","eliminar","id","nombre"],"sources":["C:\\laragon\\www\\academico-vue\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"row\">\n    <div class=\"col-lg-8 offset-lg-2\">\n      <div class=\"table-responsive\">\n        <table class=\"table table-hover custom-table text-center\">\n          <thead>\n            <tr>\n              <th>#</th>\n              <th>ID</th>\n              <th>FOTO</th>\n              <th>NOMBRE</th>\n              <th>APELLIDO</th>\n              <th>REGISTRO</th>\n            </tr>\n          </thead>\n          <tbody class=\"table-group-divider text-center\" id=\"contenido\">\n            <tr v-if=\"this.cargando\">\n              <td colspan=\"6\"><h3>Cargando...</h3></td>\n            </tr>\n            <tr v-else v-for=\"est, i in this.estudiantes\" :key=\"est.id\">\n              <td v-text=\"(i+1)\"></td>\n              <td v-text=\"est.id\"></td>\n              <td>\n                <img v-if=\"est.foto\" style=\"width: 150px !important;\" :src=\"est.foto\" class=\"img-thumbnail\" alt=\"\">\n                <img v-else style=\"width: 150px !important;\" src=\"https://cdn4.iconfinder.com/data/icons/small-n-flat/24/user-256.png\">\n              </td>\n              <td v-text=\"est.nombre\"></td>\n              <td v-text=\"est.apellido\"></td>\n              <td v-text=\"new Date(est.created_at).toLocaleDateString('en-US')\"></td>\n              <td>\n                <router-link :to=\"{path:'viewE/'+est.id}\" class=\"btn btn-info\">\n                  <i class=\"fa-solid fa-eye\"></i>\n                </router-link>\n                &nbsp;\n                <router-link :to=\"{path:'editE/'+est.id}\" class=\"btn btn-warning\">\n                  <i class=\"fa-solid fa-edit\"></i>\n                </router-link>\n                &nbsp;\n                <button class=\"btn btn-danger\" v-on:click=\"eliminar(est.id,est.nombre)\">\n                  <i class=\"fa-solid fa-trash\"></i>\n                </button>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n  import axios from 'axios';\n  import {confirmar} from '../funciones'\n\n  export default{\n    data(){\n      return{\n        estudiantes:null,\n        cargando:false\n\n      }\n    },\n    mounted(){\n      this.getEstudiantes();\n    },\n    methods:{\n      getEstudiantes(){\n        this.cargando=true;\n        axios.get('http://academicobackend.test/api/v1/estudiantes').then(\n          res =>{\n            this.estudiantes = res.data;\n            this.cargando = false; \n          }\n        );\n      },\n      eliminar(id,nombre){\n        confirmar('http://academicobackend.test/api/v1/estudiantes/',id,'Eliminar registro', 'Realmente desea eliminar a '+nombre+'?');\n        this.cargando = false;\n      }\n    }\n  }\n</script>\n"],"mappings":"AAmDE,OAAOA,KAAI,MAAO,OAAO;AACzB,SAAQC,SAAS,QAAO,cAAa;AAErC,eAAc;EACZC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,WAAW,EAAC,IAAI;MAChBC,QAAQ,EAAC;IAEX;EACF,CAAC;EACDC,OAAOA,CAAA,EAAE;IACP,IAAI,CAACC,cAAc,CAAC,CAAC;EACvB,CAAC;EACDC,OAAO,EAAC;IACND,cAAcA,CAAA,EAAE;MACd,